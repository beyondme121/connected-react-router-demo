import React from 'react'\r\nimport RouterContext from './RouterContext'\r\nimport matchPath from './matchPath'\r\nclass Route extends React.Component {\r\n  // 类组件 获取context的方式之一\r\n  static contextType = RouterContext\r\n\r\n  render() {\r\n    let { history, location, match } = this.context // 获取默认的上下文\r\n    let {\r\n      path,\r\n      component: RouteComponent,\r\n      computedMatch, // 路径匹配 Switch组件向下级组件传递的属性, 用于当匹配了路径就返回, 传递这个参数为了优化缓存\r\n      render,\r\n      children,\r\n    } = this.props\r\n    // let match = location.pathname === path\r\n    match = computedMatch\r\n      ? computedMatch\r\n      : matchPath(location.pathname, this.props)\r\n    let routeConfig = {\r\n      history,\r\n      location,\r\n    }\r\n    let element\r\n    if (match) {\r\n      // 如果路由匹配上, 更新路由的match属性对象\r\n      routeConfig.match = match\r\n      if (children) {\r\n        element = children(routeConfig) // 给children函数传递route配置对象\r\n      } else if (RouteComponent) {\r\n        // 通过props传递给Route的渲染组件, 我们还需要通过上下文\r\n        element = <RouteComponent {...routeConfig} />\r\n      } else if (render) {\r\n        element = render(routeConfig)\r\n      } else {\r\n        element = null\r\n      }\r\n    } else {\r\n      if (children) {\r\n        element = children(routeConfig)\r\n      } else {\r\n        element = null\r\n      }\r\n    }\r\n\r\n    return (\r\n      <RouterContext.Provider value={routeConfig}>\r\n        {element}\r\n      </RouterContext.Provider>\r\n    )\r\n  }\r\n}\r\nexport default Route\r\n","C:\\projects\\2020\\zf-project\\sanfeng2020router-router\\src\\history\\index.js",[],"C:\\projects\\2020\\zf-project\\sanfeng2020router-router\\src\\history\\createBrowserHistory.js",[],"C:\\projects\\2020\\zf-project\\sanfeng2020router-router\\src\\history\\createHashHistory.js",[],"C:\\projects\\2020\\zf-project\\sanfeng2020router-router\\src\\components\\Product.jsx",[],["117","118"],"C:\\projects\\2020\\zf-project\\sanfeng2020router-router\\src\\components